   "source": [
    "Esta función calcula el factorial del número k."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "def factorial (k):\n",
    "    if k==0 or k==1:\n",
    "        resultado = 1\n",
    "    else:\n",
    "        resultado = k*factorial (k-1)\n",
    "    return resultado"
   ]
  }
